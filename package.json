{
  "name": "boilerplate-nextjs-antd-less",
  "version": "2.1.0",
  "main": "index.js",
  "description": "A boilerplate for Reactjs app using nextjs, redux, redux-thunk, antd, less",
  "repository": {
    "type": "git",
    "url": "git@github.com:tientran0019/boilerplate-nextjs-antd-less.git"
  },
  "author": "Tien Tran <tientran0019@gmail.com>",
  "license": "MIT",
  "private": false,
  "keywords": [
    "reactjs",
    "boilerplate",
    "redux",
    "nextjs",
    "redux-thunk",
    "antd",
    "less"
  ],
  "scripts": {
    "clean": "rimraf .next/ out/",
    "dev": "next --port=3000",
    "build": "yarn clean && NODE_ENV=production next build",
    "build:staging": "yarn clean && NODE_ENV=staging next build",
    "export": "next export",
    "start": "next start --port=3000",
    "start:staging": "NODE_ENV=staging next start --port=3000",
    "start:pm2": "NODE_ENV=production pm2 start npm --name \"tests\" -- start",
    "build:docker-compose": "docker-compose build",
    "start:docker-compose": "docker-compose up -d",
    "build:docker-compose:staging": "docker-compose -f docker-compose.staging.yml build",
    "start:docker-compose:staging": "docker-compose -f docker-compose.staging.yml up -d",
    "build:docker": "docker build -t boilerplate-image .",
    "start:docker": "docker run --name boilerplate-container -p 3000:3000 -d boilerplate-image"
  },
  "dependencies": {
    "@ant-design/icons": "^4.6.2",
    "@babel/core": "^7.13.14",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-less": "^1.0.1",
    "antd": "^4.15.0",
    "classnames": "^2.3.1",
    "dayjs": "^1.10.4",
    "dotenv": "^8.2.0",
    "less": "^4.1.1",
    "less-vars-to-js": "^1.3.0",
    "lodash": "^4.17.21",
    "next": "^10.1.3",
    "next-compose-plugins": "^2.2.1",
    "next-fonts": "^1.5.1",
    "next-redux-wrapper": "^6.0.2",
    "nprogress": "^0.2.0",
    "null-loader": "^4.0.1",
    "numeral": "^2.0.6",
    "prop-types": "^15.7.2",
    "query-string": "^7.0.0",
    "react": "^17.0.2",
    "react-cookies": "^0.1.1",
    "react-dom": "^17.0.2",
    "react-icons": "^4.2.0",
    "react-redux": "^7.2.3",
    "react-use": "^17.2.1",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.13.8",
    "babel-eslint": "^10.1.0",
    "babel-plugin-import": "^1.13.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.22.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-import-resolver-babel-module": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "frontmatter-markdown-loader": "^3.6.2",
    "webpack": "4.45.0",
    "webpack-filter-warnings-plugin": "^1.2.1"
  }
}
